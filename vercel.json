{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://proapi.azurewebsites.net/:path*"
    },
    {
      "source": "/static/:path*",
      "destination": "/:path*"
    },
    {
      "source": "/(.*)\\.(.+)$",
      "destination": "/$1.$2"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
